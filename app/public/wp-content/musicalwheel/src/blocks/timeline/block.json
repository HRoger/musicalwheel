{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "musicalwheel/timeline",
  "version": "1.0.0",
  "title": "Timeline",
  "category": "musicalwheel-social",
  "icon": "list-view",
  "description": "Display social timeline feed with status posts, likes, and replies",
  "keywords": ["timeline", "social", "feed", "status"],
  "supports": {
    "html": false,
    "anchor": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "feedType": {
      "type": "string",
      "default": "user_feed",
      "enum": ["user_feed", "global_feed", "post_timeline", "post_wall", "post_reviews", "author_timeline"]
    },
    "orderBy": {
      "type": "string",
      "default": "latest",
      "enum": ["latest", "earliest", "most_liked", "most_discussed"]
    },
    "postsPerPage": {
      "type": "number",
      "default": 10
    },
    "showComposer": {
      "type": "boolean",
      "default": true
    },
    "enableSearch": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "musicalwheel"
}
