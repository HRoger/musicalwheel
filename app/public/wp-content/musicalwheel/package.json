{
  "name": "musicalwheel-fse",
  "version": "1.0.0",
  "description": "Modern WordPress Full Site Editing (FSE) theme for the MusicalWheel music industry ecosystem. Built with React, TypeScript, Vite, and Tailwind CSS v4.",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\""
  },
  "keywords": [
    "wordpress",
    "fse",
    "full-site-editing",
    "theme",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "author": "MusicalWheel Team",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/wordpress__block-editor": "^14.21.1",
    "@types/wordpress__blocks": "^12.5.18",
    "@types/wordpress__components": "^23.0.12",
    "@vitejs/plugin-react": "^5.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.16",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@wordpress/block-editor": "^15.7.0",
    "@wordpress/blocks": "^15.7.0",
    "@wordpress/components": "^30.7.0",
    "@wordpress/element": "^6.34.0",
    "@wordpress/i18n": "^6.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
