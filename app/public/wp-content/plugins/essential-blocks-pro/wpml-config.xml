<wpml-config>
  	<gutenberg-blocks>
		<gutenberg-block type="essential-blocks/pro-news-ticker" translate="1">
			<key name="newsLabel" />
			<key name="news">
				<key name="*">
					<key name="title" />
					<key name="content" />
					<key name="link" />
				</key>
			</key>
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-advanced-search" translate="1">
			<key name="popularKeywordsLabel" />
			<key name="taxListText" />
			<key name="loadmoreBtnText" />
			<key name="placeholderText" />
			<key name="btnText" />
			<key name="totalResultsText" />
			<key name="notFoundText" />
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-pricing-cell" translate="1">
			<xpath label="eb-mcpt-sale-badge">//div[@class="eb-mcpt-sale-badge"]/span[not(@class)]</xpath>
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-timeline-slider" translate="1">
			<key name="timelines">
				<key name="*">
					<key name="title" />
					<key name="content" />
					<key name="subheading" />
					<key name="timelineImageAlt" />
					<key name="timelineImageTitle" />
				</key>
			</key>
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-off-canvas" translate="1">
			<key name="toggleText" />
			<xpath label="eb-toggle-text">//span[@class="eb-toggle-text"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-woo-product-carousel" translate="1">
			<key name="saleText" />
			<key name="buttonText" />
		</gutenberg-block>
		<gutenberg-block type="essential-blocks/pro-fancy-chart" translate="0"/>
		<gutenberg-block type="essential-blocks/pro-data-table" translate="0"/>
	</gutenberg-blocks>
</wpml-config>
