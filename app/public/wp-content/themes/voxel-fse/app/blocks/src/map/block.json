{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "voxel-fse/map",
	"version": "1.0.0",
	"title": "Map (VX)",
	"category": "voxel-fse",
	"description": "Interactive map displaying markers from search results or current post location.",
	"keywords": ["map", "location", "markers", "google maps", "mapbox", "voxel"],
	"textdomain": "voxel-fse",
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"anchor": true,
		"className": true,
