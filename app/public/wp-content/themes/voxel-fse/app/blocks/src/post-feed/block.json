		"_comment_voxel_tab": "VoxelTab attributes for visibility and loop features",
		"visibilityBehavior": {
			"type": "string",
			"default": "show",
			"enum": [
				"show",
				"hide"
			]
		},
		"visibilityRules": {
			"type": "array",
			"default": [],
			"items": {
