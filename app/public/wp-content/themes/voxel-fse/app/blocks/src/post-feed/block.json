		},
		"carouselNavBorderType": {
			"type": "string",
			"default": ""
		},
		"carouselNavBorderWidth": {
			"type": "number"
		},
		"carouselNavBorderColor": {
			"type": "string",
			"default": ""
		},
		"carouselNavBorderRadius": {
			"type": "number"
		},
		"carouselNavBorderRadius_tablet": {
			"type": "number"
		},
		"carouselNavBorderRadius_mobile": {
			"type": "number"
		},
		"carouselNavButtonSizeHover": {
			"type": "number"
