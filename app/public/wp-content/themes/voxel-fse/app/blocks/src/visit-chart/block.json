		"visibilityBehavior": {
			"type": "string",
			"default": "show",
			"enum": [
				"show",
				"hide"
			]
		},
		"visibilityRules": {
			"type": "array",
			"default": [],
			"items": {
				"type": "object"
			}
		},
		"loopEnabled": {
			"type": "boolean",
			"default": false
		},
		"loopSource": {
