		"tabsState": {
			"type": "string",
			"default": "normal"
		},
		"tabsJustify": {
			"type": "string",
			"default": "left"
		},
		"tabsPadding": {
			"type": "object",
			"default": {}
		},
		"tabsMargin": {
			"type": "object",
			"default": {}
		},
		"tabsTypography": {
			"type": "object",
			"default": {}
