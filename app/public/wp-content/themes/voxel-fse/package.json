{
  "name": "voxel-fse",
  "version": "1.0.0",
  "description": "Full Site Editing child theme for Voxel with custom FSE blocks and DynamicTagBuilder system. Part of the MusicalWheel platform for music professionals.",
  "private": true,
  "scripts": {
    "dev": "vite build --config vite.frontend.config.js --watch",
    "dev:frontend": "vite build --config vite.frontend.config.js --watch",
    "build": "vite build --config vite.blocks.config.js && npm run build:frontend",
    "build:blocks": "vite build --config vite.blocks.config.js",
    "build:frontend": "npm run build:frontend:advanced-list && npm run build:frontend:cart-summary && npm run build:frontend:countdown && npm run build:frontend:create-post && npm run build:frontend:current-plan && npm run build:frontend:current-role && npm run build:frontend:flex-container && npm run build:frontend:gallery && npm run build:frontend:image && npm run build:frontend:listing-plans && npm run build:frontend:login && npm run build:frontend:map && npm run build:frontend:membership-plans && npm run build:frontend:messages && npm run build:frontend:navbar && npm run build:frontend:nested-accordion && npm run build:frontend:nested-tabs && npm run build:frontend:orders && npm run build:frontend:post-feed && npm run build:frontend:print-template && npm run build:frontend:product-form && npm run build:frontend:product-price && npm run build:frontend:quick-search && npm run build:frontend:review-stats && npm run build:frontend:ring-chart && npm run build:frontend:sales-chart && npm run build:frontend:search-form && npm run build:frontend:slider && npm run build:frontend:stripe-account && npm run build:frontend:term-feed && npm run build:frontend:timeline && npm run build:frontend:userbar && npm run build:frontend:visit-chart && npm run build:frontend:work-hours",
    "build:frontend:advanced-list": "vite build --config vite.frontend.config.js -- --block=advanced-list",
    "build:frontend:cart-summary": "vite build --config vite.frontend.config.js -- --block=cart-summary",
    "build:frontend:countdown": "vite build --config vite.frontend.config.js -- --block=countdown",
    "build:frontend:create-post": "vite build --config vite.frontend.config.js -- --block=create-post",
    "build:frontend:current-plan": "vite build --config vite.frontend.config.js -- --block=current-plan",
    "build:frontend:current-role": "vite build --config vite.frontend.config.js -- --block=current-role",
    "build:frontend:flex-container": "vite build --config vite.frontend.config.js -- --block=flex-container",
    "build:frontend:gallery": "vite build --config vite.frontend.config.js -- --block=gallery",
    "build:frontend:image": "vite build --config vite.frontend.config.js -- --block=image",
    "build:frontend:listing-plans": "vite build --config vite.frontend.config.js -- --block=listing-plans",
    "build:frontend:login": "vite build --config vite.frontend.config.js -- --block=login",
    "build:frontend:map": "vite build --config vite.frontend.config.js -- --block=map",
    "build:frontend:membership-plans": "vite build --config vite.frontend.config.js -- --block=membership-plans",
    "build:frontend:messages": "vite build --config vite.frontend.config.js -- --block=messages",
    "build:frontend:navbar": "vite build --config vite.frontend.config.js -- --block=navbar",
    "build:frontend:nested-accordion": "vite build --config vite.frontend.config.js -- --block=nested-accordion",
    "build:frontend:nested-tabs": "vite build --config vite.frontend.config.js -- --block=nested-tabs",
    "build:frontend:orders": "vite build --config vite.frontend.config.js -- --block=orders",
    "build:frontend:post-feed": "vite build --config vite.frontend.config.js -- --block=post-feed",
    "build:frontend:print-template": "vite build --config vite.frontend.config.js -- --block=print-template",
    "build:frontend:product-form": "vite build --config vite.frontend.config.js -- --block=product-form",
    "build:frontend:product-price": "vite build --config vite.frontend.config.js -- --block=product-price",
    "build:frontend:quick-search": "vite build --config vite.frontend.config.js -- --block=quick-search",
    "build:frontend:review-stats": "vite build --config vite.frontend.config.js -- --block=review-stats",
    "build:frontend:ring-chart": "vite build --config vite.frontend.config.js -- --block=ring-chart",
    "build:frontend:sales-chart": "vite build --config vite.frontend.config.js -- --block=sales-chart",
    "build:frontend:search-form": "vite build --config vite.frontend.config.js -- --block=search-form",
    "build:frontend:slider": "vite build --config vite.frontend.config.js -- --block=slider",
    "build:frontend:stripe-account": "vite build --config vite.frontend.config.js -- --block=stripe-account",
    "build:frontend:term-feed": "vite build --config vite.frontend.config.js -- --block=term-feed",
    "build:frontend:timeline": "vite build --config vite.frontend.config.js -- --block=timeline",
    "build:frontend:userbar": "vite build --config vite.frontend.config.js -- --block=userbar",
    "build:frontend:visit-chart": "vite build --config vite.frontend.config.js -- --block=visit-chart",
    "build:frontend:work-hours": "vite build --config vite.frontend.config.js -- --block=work-hours",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --noEmit --project tsconfig.test.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "start": "npm run dev"
  },
  "keywords": [
    "wordpress",
    "fse",
    "full-site-editing",
    "theme",
    "voxel",
    "child-theme",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "author": "MusicalWheel Team",
  "license": "Proprietary",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/google.maps": "^3.58.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.0",
    "@types/pikaday": "^1.7.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass-embedded": "^1.93.3",
    "tailwindcss": "^4.1.16",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@playwright/test": "^1.57.0",
    "@wordpress/block-editor": "^15.7.0",
    "@wordpress/blocks": "^15.7.0",
    "@wordpress/components": "^30.7.0",
    "@wordpress/element": "^6.34.0",
    "@wordpress/i18n": "^6.7.0",
    "flatpickr": "^4.6.13",
    "leaflet": "^1.9.4",
    "nouislider": "^15.8.1",
    "pikaday": "^1.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "yet-another-react-lightbox": "^3.28.0"
  }
}
